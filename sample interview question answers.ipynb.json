    "def fibFunc(user_input):\n",
    "    user_input = float(user_input)\n",
    "    if  user_input<=0 or user_input.is_integer()==False:\n",
    "        return \"invalid input must be positive integer\"\n",
    "    elif user_input == 1 or user_input==2:\n",
    "        return 0\n",
    "    elif user_input==2:\n",
    "        return  1\n",
    "    else:\n",
    "        return fibFunc(user_input-1)+fibFunc(user_input-2)\n",
    "        \n",
    "    \n"
   ]
